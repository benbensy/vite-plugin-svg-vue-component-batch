{
  "name": "vite-plugin-svg-vue-component-batch",
  "version": "0.0.5",
  "description": "Vite plugin for importing svg vue components from a directory.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "types": "./dist/svg-components.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "vue",
    "plugin"
  ],
  "author": "indusy",
  "license": "MIT",
  "packageManager": "pnpm@10.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/benbensy/vite-plugin-svg-vue-component-batch.git"
  },
  "bugs": {
    "url": "https://github.com/benbensy/vite-plugin-svg-vue-component-batch/issues"
  },
  "homepage": "https://github.com/benbensy/vite-plugin-svg-vue-component-batch#readme",
  "dependencies": {
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.0.0",
    "transliteration": "^2.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "vite": "^4.2.2",
    "vue": "^3.2.0"
  }
}